<Window x:Class="HtmlParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HtmlParser"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ListBox x:Name="listbox" HorizontalAlignment="Left" Height="319" VerticalAlignment="Top" Width="382"/>
        <Label Content="Start Point" HorizontalAlignment="Left" Margin="387,0,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtstart" HorizontalAlignment="Left" Height="23" Margin="387,31,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblendpoint" Content="End Point" HorizontalAlignment="Left" Margin="387,87,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtendpoint" HorizontalAlignment="Left" Height="23" Margin="387,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnStart" Content="Start" HorizontalAlignment="Left" Margin="407,186,0,0" VerticalAlignment="Top" Width="75" Click="btnStart_Click"/>
        <Button x:Name="btnAbort" Content="Abort" HorizontalAlignment="Left" Margin="407,240,0,0" VerticalAlignment="Top" Width="75" Click="btnAbort_Click"/>

    </Grid>
</Window>
